import java.util.*;

class Result {

    public static List<Integer> detectHighLoadWindows(List<Integer> loads, int windowSize, int threshold) {
        List<Integer> result = new ArrayList<>();
        int n = loads.size();

        if (windowSize > n) return result;  

        long windowSum = 0;

        
        for (int i = 0; i < windowSize; i++) {
            windowSum += loads.get(i);
        }

        
        if ((windowSum / (double) windowSize) > threshold) {
            result.add(windowSize - 1);
        }

        
        for (int i = windowSize; i < n; i++) {
            windowSum += loads.get(i);
            windowSum -= loads.get(i - windowSize);

            if ((windowSum / (double) windowSize) > threshold) {
                result.add(i);
            }
        }

        return result;
    }
}

public class Solution {
    public static void main(String[] args) {
        List<Integer> loads = Arrays.asList(8, 14, 5, 17);
        int windowSize = 2;
        int threshold = 10;

        List<Integer> ans = Result.detectHighLoadWindows(loads, windowSize, threshold);
        System.out.println(ans); 
    }
}